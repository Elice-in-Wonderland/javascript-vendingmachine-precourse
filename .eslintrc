{
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-base",
    "airbnb-typescript/base",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ], 
  "parserOptions": {"project": ["./tsconfig.json"] },
  "rules": {
    "class-methods-use-this": "off"
  }
}